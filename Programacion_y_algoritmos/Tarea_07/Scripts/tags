Wav	Functions/wave.h	/^} Wav;$/;"	t	typeref:struct:WAV_fotmat
_FUNC	Makefile	/^_FUNC=	wave.c$/;"	m
data	Functions/wave.h	/^    Data_t data;$/;"	m	struct:WAV_fotmat
