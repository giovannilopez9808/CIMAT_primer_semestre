!_TAG_FILE_FORMAT	2	/extended format; --format=1 will not append ;" to lines/
!_TAG_FILE_SORTED	1	/0=unsorted, 1=sorted, 2=foldcase/
!_TAG_PROGRAM_AUTHOR	Darren Hiebert	/dhiebert@users.sourceforge.net/
!_TAG_PROGRAM_NAME	Exuberant Ctags	//
!_TAG_PROGRAM_URL	http://ctags.sourceforge.net	/official site/
!_TAG_PROGRAM_VERSION	5.9~svn20110310	//
CC	Makefile	/^CC=gcc$/;"	m
CFLAGS	Makefile	/^CFLAGS= -Wall -Wextra -Werror -O3 -pedantic -ansi -std=c11$/;"	m
EXEC	Makefile	/^EXEC= main.out$/;"	m
FOBJS	Makefile	/^FOBJS=$(FUNC:.c=.o)$/;"	m
FUNC	Makefile	/^FUNC=$(patsubst %,$(IDIR)%,$(_FUNC))$/;"	m
IDIR	Makefile	/^IDIR=Modules\/$/;"	m
LDFLAGS	Makefile	/^LDFLAGS=-std=c11 -lm$/;"	m
OBJS	Makefile	/^OBJS=$(SOURCES:.c=.o)$/;"	m
SOURCES	Makefile	/^SOURCES=main.c$/;"	m
_FUNC	Makefile	/^_FUNC=	functions.c \\$/;"	m
f_x	main.c	/^double f_x(double x)$/;"	f
functions_H	Modules/functions.h	2;"	d
h_function	Modules/functions.c	/^double h_function(double (*f)(double), double x)$/;"	f
main	main.c	/^int main()$/;"	f
newton_H	Modules/newton.h	2;"	d
newton_method	Modules/newton.c	/^void newton_method(double (*f)(double), double tol_x, double tol_f, double x0)$/;"	f
obtain_derivative	Modules/functions.c	/^double obtain_derivative(double (*f)(double), double x)$/;"	f
obtain_h_derivative	Modules/functions.c	/^double obtain_h_derivative(double (*f)(double), double x)$/;"	f
obtain_new_x_newton	Modules/newton.c	/^double obtain_new_x_newton(double (*f)(double), double x)$/;"	f
stopping_criteria	Modules/newton.c	/^double stopping_criteria(double x0, double x1)$/;"	f
